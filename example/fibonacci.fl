let l = 1;
let r = 1;

for (let i = 0; i < 15; i = i + 1) {
    let new_r = l + r;
    l = r;
    r = new_r;
    print new_r;
}

fun fib(n) {
    if (n < 2) {
        return n;
    } else {
        let l = fib(n-2);
        let r = fib(n-1);
        return l + r;
    }
}

let result = fib(10);
print "Fib of 10 is";
print result;

